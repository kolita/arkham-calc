<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.kolita.arkhamcalc"
      android:versionName="1.4" android:versionCode="5">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".ArkhamCalc"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArkhamCalcHelp">
            <intent-filter>
                <action android:name="android.intent.action.ARKHAMCALCHELP"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>


<uses-sdk android:minSdkVersion="4" android:targetSdkVersion="4"></uses-sdk>
</manifest> 